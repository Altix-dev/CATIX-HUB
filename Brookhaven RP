local w4 = library:CreateWindow("Auto Spawn Cars")
local w4 = w4:CreateFolder("Auto Spawn Cars")


w4:Toggle("Auto Horse",function(bool)
		shared.toggle1 = bool
	end)
		w4:Toggle("Auto Ambulance",function(bool)
		shared.toggle2 = bool
	end)


	w4:Toggle("Auto Bus",function(bool)
