   if shared.toggle4 then   
             game:GetService("ReplicatedStorage").RemoteEvents.Car:FireServer("PickingCar","Bus")
game:GetService("ReplicatedStorage").RemoteEvents.Car:FireServer("PickingCar","Bus")


       end
 
      if shared.toggle5 then   
        game:GetService("ReplicatedStorage").RemoteEvents.Car:FireServer("PickingCar","Van")
