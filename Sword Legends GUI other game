    if arg then
        _G.b = true 
        while wait(0) and _G.b do
            local A_1 = 2
local Event = game:GetService("ReplicatedStorage").ShopBuy
Event:FireServer(A_1)

  local A_1 = 3
local Event = game:GetService("ReplicatedStorage").ShopBuy
Event:FireServer(A_1)

      local A_1 = 4
local Event = game:GetService("ReplicatedStorage").ShopBuy
Event:FireServer(A_1)

      local A_1 = 5
local Event = game:GetService("ReplicatedStorage").ShopBuy
Event:FireServer(A_1)

      local A_1 = 6
local Event = game:GetService("ReplicatedStorage").ShopBuy
Event:FireServer(A_1)

      local A_1 = 7
local Event = game:GetService("ReplicatedStorage").ShopBuy
Event:FireServer(A_1)

      local A_1 = 8
local Event = game:GetService("ReplicatedStorage").ShopBuy
Event:FireServer(A_1)

      local A_1 = 9
local Event = game:GetService("ReplicatedStorage").ShopBuy
Event:FireServer(A_1)

      local A_1 = 10
local Event = game:GetService("ReplicatedStorage").ShopBuy
