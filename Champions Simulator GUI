game.Players.LocalPlayer.character.HumanoidRootPart.CFrame = CFrame.new(-22.9144306, 48.9636803, -5422.31543)
end)

AutoTab:CreateToggle("Auto Buy Gloves", function(arg) --the (arg) is if the checkbox is toggled or not
    if arg then
        _G.b = true 
