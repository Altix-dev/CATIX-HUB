    end
end)

AutoTab:CreateToggle("Auto Buy Bronze Eggs", function(arg) --the (arg) is if the checkbox is toggled or not
    if arg then
        _G.Basic = true 
        while wait(0) and _G.Basic do
           local args = {
    [1] = "Starter World Bronze",
    [2] = 1
}

game:GetService("ReplicatedStorage").Events.BuyEggPromptEvent:FireServer(unpack(args))

        end
    else
        _G.Basic = false
    end
end)

