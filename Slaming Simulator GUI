AutoTab:CreateToggle("Auto Magma Bosses", function(arg) --the (arg) is if the checkbox is toggled or not
    if arg then
        _G.c = true 
        while wait(0) and _G.c do


wait(0.1) --change time if you want
local boss = game.workspace.Bosses:FindFirstChildOfClass("Model").Head
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = boss.CFrame
        end
    else
        _G.c= false
    end
end)
