AutoTab:CreateToggle("Speed Hack 100", function(arg) --the (arg) is if the checkbox is toggled or not
    if arg then
        _G.Basic = true 
        while wait(0) and _G.Basic do
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 100
        end
    else
        _G.Basic = false
    end
end)

eggs3:CreateToggle("Auto Buy Starter Eggs", function(arg) --the (arg) is if the checkbox is toggled or not
    if arg then
        _G.Basic = true 
        while wait(0) and _G.Basic do
                  local args = {
    [1] = game:GetService("Players")["quentin453"],
    [2] = "Starter",
    [3] = true,
}

game:GetService("ReplicatedStorage").KeyBind:InvokeServer(unpack(args))
        end
    else
        _G.Basic = false
    end
end)

