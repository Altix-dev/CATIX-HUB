eggs:CreateToggle("Auto Buy Normal Eggs", function(arg) --the (arg) is if the checkbox is toggled or not
    if arg then
_G.f = true 
        while wait() and _G.f do
    
local args = {
    [1] = "Buy",
    [2] = {
        [1] = "Egg",
        [2] = "Normal",
    },
}

game:GetService("ReplicatedStorage").Events.RemoteEvent:FireServer(unpack(args))
end
    else
        _G.f = false
         
         end
end)

