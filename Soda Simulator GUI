--https://www.roblox.com/games/5281022480/Beta-Soda-Simulator
local ui = loadstring(game:HttpGet("https://pastebin.com/raw/zQWg2eeL"))()

-- UI:Init(parent)
-- Init:AddTab(title)
-- AddTab:AddSection(title)
-- AddSection:AddButton(text, callback)
-- AddSection:AddSlider(text, default, callback)
-- AddSection:AddKeybind(text, default, callback)
-- AddSection:AddToggle(text, default, callback)

local mainUI = ui:Init(game:GetService("CoreGui"))
local homeTab = mainUI:AddTab("Hacks")
local eggs = mainUI:AddTab("Eggs x1")
local eggs2 = mainUI:AddTab("Eggs x3")
local farmSection = homeTab:AddSection("Auto Farmers")
local farmMobs = farmSection:AddToggle("Auto Clicks", false, function()
_G.b = true 
while wait() and _G.b do
    for i = 1, 100 do 
        game:GetService("ReplicatedStorage").Events["Energy2Event"]:FireServer()
        game:GetService("ReplicatedStorage").Events["Energy2Event"]:FireServer()
        game:GetService("ReplicatedStorage").Events["Energy2Event"]:FireServer()
    end 
end
end)
local farmBosses = farmSection:AddToggle("Auto Upgrades", false, function()
_G.c = true 
while wait() and _G.c do
   
  game:GetService("ReplicatedStorage").Events.UpgradeEvent:FireServer()
    end 
end)
local farmBosses = farmSection:AddToggle("Auto Sell", false, function()
_G.d = true
while wait() and _G.d do
game:GetService("ReplicatedStorage").Events.TradeEvent:FireServer()
    end 
end)
local farmBosses = farmSection:AddToggle("Auto Collect Coins", false, function()
_G.o = true
while wait(0.5) and _G.o do
local children = game.Workspace:GetChildren()
    for _, child in pairs(children) do
    for _, child in pairs(child:GetChildren()) do
    table.insert(children, child)
end
    if child:IsA("BasePart") and child.Name == "Chest" then
        child.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame  
end
end 
for i, v in pairs(game:GetService("Workspace").Treasure.Coins:GetChildren("NormalCoin")) do
    v.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
end
end
end)
    
local farmSection = eggs:AddSection("Auto Eggs x1")
local farmBosses = farmSection:AddToggle("Auto Buy Dominus Eggs", false, function()
_G.egg = true
while wait() and _G.egg do
local args = {
    [1] = game:GetService("Players")["LocalPlayer"],
    [2] = "Common",
    [3] = false,
}

game:GetService("ReplicatedStorage").KeyBind:InvokeServer(unpack(args))
    end 
end)
local farmBosses = farmSection:AddToggle("Auto Buy Common Eggs", false, function()
_G.egg1 = true
while wait() and _G.egg1 do
local args = {
    [1] = game:GetService("Players")["LocalPlayer"],
    [2] = "Rare",
    [3] = false,
}

game:GetService("ReplicatedStorage").KeyBind:InvokeServer(unpack(args))

    end 
end)
local farmSection = eggs2:AddSection("Auto Eggs x3")
local farmBosses = farmSection:AddToggle("Auto Buy Dominus Eggs", false, function()
_G.egg = true
while wait() and _G.egg do
local args = {
    [1] = game:GetService("Players")["LocalPlayer"],
    [2] = "Common",
    [3] = true,
}

game:GetService("ReplicatedStorage").KeyBind:InvokeServer(unpack(args))
    end 
end)
local farmBosses = farmSection:AddToggle("Auto Buy Common Eggs", false, function()
_G.egg1 = true
while wait() and _G.egg1 do
local args = {
    [1] = game:GetService("Players")["LocalPlayer"],
    [2] = "Rare",
    [3] = true,
}

game:GetService("ReplicatedStorage").KeyBind:InvokeServer(unpack(args))

    end 
end)
