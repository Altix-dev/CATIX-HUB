AutoTab:CreateToggle("Speed Hack 100", function(arg) --the (arg) is if the checkbox is toggled or not
    if arg then
        _G.Basic = true 
        while wait(0) and _G.Basic do
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 100
        end
    else
        _G.Basic = false
    end
end)

AutoTab:CreateToggle("Auto Click", function(arg) --the (arg) is if the checkbox is toggled or not
    if arg then
        _G.LULU = true 
        while wait(0) and _G.LULU do
            if game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool") then
            game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool"):Activate()
    end
end

    else
        _G.LULU = false
    end
end)

AutoTab:CreateButton("Destroy Gui :D", function() --you dont need "arg" for a button
game:GetService("CoreGui").uiui:Destroy()
end)

teleports:CreateDropDown("Islands World 1", {"Spawn", "Islands 1", "Islands 2", "Islands 3", "Islands 4"}, function(arg) --the (arg) is the option you choose 
    if arg == "Spawn" then
        game.Players.LocalPlayer.character.HumanoidRootPart.CFrame = CFrame.new(168.053314, 23.253582, -85.6360474)
    elseif arg == "Islands 1" then
        game.Players.LocalPlayer.character.HumanoidRootPart.CFrame = CFrame.new(-21.463356, 941.92981, -53.7636833)
    elseif arg == "Islands 2" then
        game.Players.LocalPlayer.character.HumanoidRootPart.CFrame = CFrame.new(-46.9555321, 2653.21948, -30.7914066)
    elseif arg == "Islands 3" then
        game.Players.LocalPlayer.character.HumanoidRootPart.CFrame = CFrame.new(-77.4848557, 4960.21826, -33.9025917)
    elseif arg == "Islands 4" then
        game.Players.LocalPlayer.character.HumanoidRootPart.CFrame = CFrame.new(-29.3737602, 8502.2168, -45.3552628)
    end
end)

teleports:CreateDropDown("Islands World 2", {"Spawn", "Islands 1", "Islands 2", "Islands 3"}, function(arg) --the (arg) is the option you choose 
    if arg == "Spawn" then
        game.Players.LocalPlayer.character.HumanoidRootPart.CFrame = CFrame.new(-16676.9941, 81.8307724, 85182.1484)
    elseif arg == "Islands 1" then
        game.Players.LocalPlayer.character.HumanoidRootPart.CFrame = CFrame.new(-16610.6973, 2161.91089, 85300.6953)
    elseif arg == "Islands 2" then
        game.Players.LocalPlayer.character.HumanoidRootPart.CFrame = CFrame.new(-46.9555321, 2653.21948, -30.7914066)
    elseif arg == "Islands 3" then
        game.Players.LocalPlayer.character.HumanoidRootPart.CFrame = CFrame.new(-16552.9355, 6120.62744, 85205.7656)
    end
end)

