eggs:CreateToggle("Auto Buy Shiny Eggs", function(arg) --the (arg) is if the checkbox is toggled or not
    if arg then
        _G.Basic = true 
        while wait(0) and _G.Basic do
local args = {
    [1] = "OpenEgg",
    [2] = 1,
    [3] = "Shiny902",
}

game:GetService("ReplicatedStorage").Communication.RE:FireServer(unpack(args))
        end
    else
        _G.Basic = false
    end
end)

