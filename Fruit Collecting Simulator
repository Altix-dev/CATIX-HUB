Upgrades:CreateToggle("GemUpgrade", function(arg) --the (arg) is if the checkbox is toggled or not
    if arg then
        _G.Basic = true 
        while wait(0) and _G.Basic do
            local args = {
    [1] = "GemUpgrade",
    [2] = "Coins",
    [3] = game:GetService("Players").LocalPlayer
}

game:GetService("ReplicatedStorage").Events.Upgrade:FireServer(unpack(args))

        end
    else
        _G.Basic = false
    end
end)

