AutoTab:CreateToggle("Auto Click", function(arg) --the (arg) is if the checkbox is toggled or not
    if arg then
        _G.c = true 
        while wait(0) and _G.c do
    local args = {
    [1] = "Click",
    [2] = {
    },
}

game:GetService("ReplicatedStorage").Events.Npc:FireServer(unpack(args))
        end
    else
        _G.c = false
    end
end)
AutoTab:CreateToggle("Auto Rebirth", function(arg) --the (arg) is if the checkbox is toggled or not
    if arg then
        _G.b = true 
        while wait(0) and _G.b do
       local args = {
    [1] = "Rebirth",
}

game:GetService("ReplicatedStorage").Events.RemoteEvent:FireServer(unpack(args))
        end
    else
        _G.b= false
    end
end)



